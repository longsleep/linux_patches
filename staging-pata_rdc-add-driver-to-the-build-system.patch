 drivers/staging/Kconfig           |    2 ++
 drivers/staging/Makefile          |    2 +-
 drivers/staging/pata_rdc/Kconfig  |    6 ++++++
 drivers/staging/pata_rdc/Makefile |    2 ++
 4 files changed, 11 insertions(+), 1 deletion(-)

--- a/drivers/staging/Kconfig
+++ b/drivers/staging/Kconfig
@@ -123,5 +123,7 @@ source "drivers/staging/cpc-usb/Kconfig"
 
 source "drivers/staging/mrst_nand/Kconfig"
 
+source "drivers/staging/pata_rdc/Kconfig"
+
 endif # !STAGING_EXCLUDE_BUILD
 endif # STAGING
--- a/drivers/staging/Makefile
+++ b/drivers/staging/Makefile
@@ -44,4 +44,4 @@ obj-$(CONFIG_USB_SERIAL_QUATECH2)	+= ser
 obj-$(CONFIG_VT6655)		+= vt6655/
 obj-$(CONFIG_USB_CPC)		+= cpc-usb/
 obj-$(CONFIG_MRST_NAND)		+= mrst_nand/
-
+obj-$(CONFIG_RDC_17F3101X)	+= pata_rdc/
--- /dev/null
+++ b/drivers/staging/pata_rdc/Kconfig
@@ -0,0 +1,6 @@
+config RDC_17F3101X
+	tristate "RDC_17F3101X IDE support"
+	depends on PCI && ATA && ATA_SFF
+	---help---

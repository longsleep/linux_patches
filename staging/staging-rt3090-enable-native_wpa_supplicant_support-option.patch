---
 drivers/staging/rt3090/config.mk |    2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

--- a/drivers/staging/rt3090/config.mk
+++ b/drivers/staging/rt3090/config.mk
@@ -20,7 +20,7 @@ HAS_APCLI=n

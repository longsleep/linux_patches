+			DBGPRINT(RT_DEBUG_TRACE,
+				("%s, release Mcu Lock\n", __func__));
 			RTMP_SEM_LOCK(&pAd->McuCmdLock);
 			pAd->brt30xxBanMcuCmd = FALSE;

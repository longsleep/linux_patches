@@ -57,24 +57,10 @@
 	}                                                                 \
     }
 #endif
-#ifndef ASSERTMSG
-#define ASSERTMSG(a,msg)                                                  \
-    {                                                                     \
-	if (!(a)) {                                                       \
-		printk(KERN_ERR "ASSERT() Failure: file %s, function %s" \
